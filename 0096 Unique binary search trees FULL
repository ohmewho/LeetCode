class Solution {
public:
    int numTrees(int n) {
        long long c = 1;
        for(int i = 0; i < n; i++) c = c * 2 * (2 * i + 1) / (i + 2);
        return c;
    }
};

int stringToInteger(string input) {
    return stoi(input);
}

int main() {
    string line;
    while (getline(cin, line)) {
        int n = stringToInteger(line);
        
        int ret = Solution().numTrees(n);

        string out = to_string(ret);
        cout << out << endl;
    }
    return 0;
}
