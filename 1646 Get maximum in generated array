class Solution {
public:
    int solve(int n) {
        if(n < 2) return n;

        int ans = 0;
        int nums[n + 1];
        nums[0] = 0;
        nums[1] = 1;

        for(int i = 2; i < n + 1; i++) {
            if(i % 2 == 0) nums[i] = nums[i / 2];
            else nums[i] = nums[i / 2] + nums[i / 2 + 1];
            ans = max(ans, nums[i]);
        }

        return ans;
    }

    int getMaximumGenerated(int n) {
        return solve(n);
    }
};
